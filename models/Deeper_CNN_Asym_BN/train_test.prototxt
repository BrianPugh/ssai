layer {
  name: "conv17"
  type: "Convolution"
  bottom: "relu16"
  top: "conv17"
  convolution_param {
    num_output: 128
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "bn18"
  type: "BN"
  bottom: "conv17"
  top: "bn18"
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  bn_param {
    scale_filler {
      type: "constant"
      value: 1
    }
    shift_filler {
      type: "constant"
      value: 0.001
    }
  }
}
layer {
  name: "relu19"
  type: "ReLU"
  bottom: "bn18"
  top: "relu19"
}
layer {
  name: "fc20"
  type: "InnerProduct"
  bottom: "relu19"
  top: "fc20"
  inner_product_param {
    num_output: 4096
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "relu21"
  type: "ReLU"
  bottom: "fc20"
  top: "relu21"
}
layer {
  name: "fc22"
  type: "InnerProduct"
  bottom: "relu21"
  top: "fc22"
  inner_product_param {
    num_output: 768
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "reshape23"
  type: "Reshape"
  bottom: "fc22"
  top: "reshape23"
  reshape_param {
    channels: 3
    height: 16
    width: 16
  }
}
layer {
  name: "predict_loss"
  type: "SoftmaxCrossEntropyLoss"
  bottom: "reshape23"
  bottom: "label"
  top: "predict_loss"
  loss_weight: 1
  softmax_cross_entropy_loss_param {
    weights: 0
    weights: 1.5
    weights: 1.5
  }
}
layer {
  name: "precision_recall_loss"
  type: "PrecisionRecallLoss"
  bottom: "reshape23"
  bottom: "label"
  top: "error_rate"
  include: { phase: TEST }
}
