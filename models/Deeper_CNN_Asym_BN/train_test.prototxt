    }
  }
}
layer {
  name: "relu13"
  type: "ReLU"
  bottom: "bn12"
  top: "relu13"
}
layer {
  name: "conv14"
  type: "Convolution"
  bottom: "relu13"
  top: "conv14"
  convolution_param {
    num_output: 128
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "bn15"
  type: "BN"
  bottom: "conv14"
  top: "bn15"
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  bn_param {
    scale_filler {
      type: "constant"
