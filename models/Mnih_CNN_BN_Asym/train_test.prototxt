name: "Mnih_CNN_BN_Asym"
layer {
  name: "data"
  type: "PatchBasedSegmentationData"
  top: "data"
  top: "label"
  patch_based_segmentation_data_param {
    source: "../../data/mass_merged/lmdb/train.lmdb"
    batch_size: 128
    rand_skip: 128
    data_channels: 3
    data_height: 64
    data_width: 64
    label_channels: 3
    label_height: 16
    label_width: 16
    rotation: true
    flip: true
  }
  include: { phase: TRAIN }
}
layer {
  name: "data"
  type: "PatchBasedSegmentationData"
  top: "data"
  top: "label"
  patch_based_segmentation_data_param {
    source: "../../data/mass_merged/lmdb/test.lmdb"
    batch_size: 128
    rand_skip: 128
    data_channels: 3
    data_height: 64
    data_width: 64
    label_channels: 3
    label_height: 16
    label_width: 16
    rotation: true
    flip: true
  }
  include: { phase: TEST }
}
layer {
  name: "mvn1"
  type: "MVN"
  bottom: "data"
  top: "mvn1"
  mvn_param {
    normalize_variance: true
    across_channels: false
  }
}
layer {
  name: "conv2"
  type: "Convolution"
  bottom: "mvn1"
  top: "conv2"
  convolution_param {
    num_output: 64
    kernel_size: 16
    stride: 4
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "bn3"
  type: "BN"
  bottom: "conv2"
  top: "bn3"
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  bn_param {
    scale_filler {
      type: "constant"
      value: 1
    }
    shift_filler {
      type: "constant"
      value: 0.001
    }
  }
}
layer {
  name: "relu4"
  type: "ReLU"
  bottom: "bn3"
  top: "relu4"
}
layer {
  name: "pool5"
  type: "Pooling"
  bottom: "relu4"
  top: "pool5"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 1
  }
}
layer {
  name: "conv6"
  type: "Convolution"
  bottom: "pool5"
  top: "conv6"
  convolution_param {
    num_output: 112
    kernel_size: 4
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "bn7"
  type: "BN"
  bottom: "conv6"
  top: "bn7"
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  bn_param {
    scale_filler {
      type: "constant"
      value: 1
    }
    shift_filler {
      type: "constant"
      value: 0.001
    }
  }
}
layer {
  name: "relu8"
  type: "ReLU"
  bottom: "bn7"
  top: "relu8"
}
layer {
  name: "conv9"
  type: "Convolution"
  bottom: "relu8"
  top: "conv9"
  convolution_param {
    num_output: 80
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "bn10"
  type: "BN"
  bottom: "conv9"
  top: "bn10"
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  param {
    lr_mult: 1.00001
    decay_mult: 0
  }
  bn_param {
    scale_filler {
      type: "constant"
      value: 1
    }
    shift_filler {
      type: "constant"
      value: 0.001
    }
  }
}
layer {
  name: "relu11"
  type: "ReLU"
  bottom: "bn10"
  top: "relu11"
}
layer {
  name: "fc12"
  type: "InnerProduct"
  bottom: "relu11"
  top: "fc12"
  inner_product_param {
    num_output: 4096
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "relu13"
  type: "ReLU"
  bottom: "fc12"
  top: "relu13"
}
layer {
  name: "fc14"
  type: "InnerProduct"
  bottom: "relu13"
  top: "fc14"
  inner_product_param {
    num_output: 768
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "reshape15"
  type: "Reshape"
  bottom: "fc14"
  top: "reshape15"
  reshape_param {
    channels: 3
    height: 16
    width: 16
  }
}
layer {
  name: "predict_loss"
  type: "SoftmaxCrossEntropyLoss"
  bottom: "reshape15"
  bottom: "label"
  top: "predict_loss"
  loss_weight: 1
  softmax_cross_entropy_loss_param {
    weights: 0
    weights: 1.5
    weights: 1.5
  }
}
layer {
  name: "precision_recall_loss"
  type: "PrecisionRecallLoss"
  bottom: "reshape15"
  bottom: "label"
  top: "error_rate"
  include: { phase: TEST }
}
