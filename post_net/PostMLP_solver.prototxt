net: "PostMLP_train_test.prototxt"
test_iter: 100
test_interval: 10000

solver_type: SGD
base_lr: 0.0005
lr_policy: "fixed"
momentum: 0.9
weight_decay: 0.0005

display: 100
max_iter: 2000000

snapshot: 10000
snapshot_prefix: "postnet_snapshots/PostMLP"

solver_mode: GPU
device_id: 2

random_seed: 1701
